{
  "name": "auto-external-packages",
  "version": "1.0.0",
  "description": "external package configs",
  "main": "lib/index.js",
  "scripts": {
    "precommit": "npm run lint-staged",
    "lint-staged": "lint-staged",
    "prepublishOnly": "npm run build",
    "clean": "rimraf ./lib",
    "build": "npm run clean && tsc",
    "test": "",
    "lint": "tslint --project ./tsconfig.json",
    "lint:fix": "tslint --fix --project ./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:umijs/auto-external-packages.git"
  },
  "keywords": [
    "client"
  ],
  "author": "ikobe",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^10.12.1",
    "@types/uuid": "^3.4.4",
    "lint-staged": "^8.0.4",
    "rimraf": "^2.6.3",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.ts": "npm run lint"
    },
    "ignore": [
      "src/typings/**.ts"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.11"
  }
}
